Xcursor.theme: Vanilla-DMZ

Xft.antialias: 1
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.rgba: rgb


xterm*termName: xterm-256color
xterm*locale: true
xterm*metaSendsEscape: true
xterm*toolBar: false

xterm*saveLines: 4096

! Treat URLs as words to ease selection
XTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48

urxvt*font: xft:Bitstream Vera Sans Mono:pixelsize=11
!URxvt*boldFont: xft:Arial code r:style=Bold:pixelsize=11
!URxvt*italicFont: xft:Arial code r:style=Italic:pixelsize=11
!URxvt*boldItalicFont: xft:Arial code r:style=boldItalic:pixelsize=11


!URxvt*termName: rxvt-256color
URxvt*saveLines: 32767
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*loginShell: true
!URxvt*tintColor: #121212
!URxvt*shading: 40
URxvt*termName: rxvt-unicode
!URxvt*inheritPixmap: true
!URxvt*cutchars: "()*,<>[]{}|"
URxvt*secondaryScroll: true
urxvt*perl-lib: /usr/lib/urxvt/perl/
urxvt*perl-ext-common: default,matcher
URxvt*urlLauncher: /usr/bin/jumanji
URxvt*matcher.button: 1
URxvt*matcher.pattern.1: \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt*visualBell: true
URxvt*keysym.C-u: perl:matcher
!URxvt*borderColor: #1A1A1A
URxvt*borderless: true
!Urxvt*externalBorder: 0
!Urxvt*background: #fffef6

!Colors
*foreground: #FFFFFF
*background: #000000
URxvt*transparent: true
URxvt*shading: 20
*color0: #363636
*color8: #424242
*color1: #C37561
*color9:  #D19485
*color2: #A0A57E
*color10: #7f8264
*color3: #b58b62
*color11: #DEBC9C
*color4: #1d2b41
*color12: #98A1B9
*color5: #AB716D
*color13: #b4d2c8
*color6: #98B9B1
*color14: #769293
*color7: #9ba0a7
*color15: #6e7383
